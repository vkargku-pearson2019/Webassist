<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Role Selection : WebAssist">

	<test name="Role Selection">
		<parameter name="testDescription"
			value="Select Different Roles Test" />
		<classes>
			<class name="webassist.login.LoginTest">
				<methods>
					<include name="loginWebAssistCorrect" />
				</methods>
			</class>
			<class name="webassist.role.RoleTest">
				<methods>
					<include name="enterRoleName" />
				</methods>
			</class>
		</classes>
	</test>
</suite>
